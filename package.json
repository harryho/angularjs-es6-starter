{
    "name": "angularjs-webpack-es6-starter",
    "version": "1.0.0",
    "description": "A quick start for Angular 1.x made with Webpack including font-awesome, bootstrap",
    "scripts": {
        "build": "rimraf dist && webpack --bail --progress --profile",
        "server": "webpack-dev-server --history-api-fallback --inline --progress",
